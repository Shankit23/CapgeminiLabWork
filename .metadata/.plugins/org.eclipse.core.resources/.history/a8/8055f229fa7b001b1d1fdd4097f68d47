package Lab1;

public class Exercise1 {
	public static void main(String args[]) {
		int number=0;
		System.out.println("Enter a number:" +number);
        Sum(number);
	}
     static void Sum(int n) {
 		while(n>0){
			int x=n%10;
			int sum=sum+(x*x*x);
			n=n/10;
		}
 		System.out.println("Sum of the cubes of the digits of your number:"+sum);
     }
}
